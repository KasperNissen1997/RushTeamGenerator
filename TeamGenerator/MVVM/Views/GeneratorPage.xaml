<Page x:Class="TeamGenerator.MVVM.Views.GeneratorPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TeamGenerator.MVVM.Views"
      mc:Ignorable="d" 
      d:DesignHeight="500" d:DesignWidth="850"
      Title="GeneratorPage">

    <Grid x:Name="GenPage" Background="#FFF9F9F9">
        <ListBox x:Name="Iteration_Show" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,0,668,0" Background="#FFF19393"/>
        <ListBox x:Name="Teams_Show" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="132,0,500,0" Background="#FFE02222"/>
        <Button x:Name="GenTeam" Content="Generat" HorizontalAlignment="Left" Margin="513,404,0,0" VerticalAlignment="Top" Height="45" Width="142" Click="Gen_Team_Click"/>
        <Grid>
            <Border x:Name="Border_Frame" BorderBrush="#FF4365CC" BorderThickness="2,2,2,2" Margin="351,0,0,124">
                <ListBox x:Name="Players_Show" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="142,93,143,93"/>
            </Border>
        </Grid>
    </Grid>
</Page>
