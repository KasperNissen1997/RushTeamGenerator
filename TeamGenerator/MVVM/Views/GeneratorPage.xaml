<Page x:Class="TeamGenerator.MVVM.Views.GeneratorPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TeamGenerator.MVVM.Views"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="GeneratorPage">

    <Grid x:Name="GenPage" Background="#FFF9F9F9">
        <ListBox d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,0,668,0" Background="#FFF19393"/>
        <ListBox d:ItemsSource="{d:SampleData ItemCount=5}" Margin="132,0,500,0" Background="#FFE02222"/>
        <Button x:Name="GenTeam" Content="Generat" HorizontalAlignment="Left" Margin="489,383,0,0" VerticalAlignment="Top" Height="42" Width="141" Click="Gen_Team_Click"/>
        <Grid>
            <Border x:Name="Border_Frame" BorderBrush="#FF4365CC" BorderThickness="2,2,2,2" Margin="300,0,0,97"/>
        </Grid>
    </Grid>
</Page>
